                margin-right: 10px;
            }

            .span4 .img-left {
                float: left;
            }

            .span4 .img-right {
                float: right;
            }
        style.
            * {
                box-sizing: border-box;
            }

            /* Create four equal columns that floats next to each other */
            .column {
                float: left;
                width: 25%;
                padding: 25px;
                height: 1000px; /* Should be removed. Only for demonstration */
            }

            /* Clear floats after the columns */
            .row:after {
                content: "";
                display: table;
                clear: both;
            }

